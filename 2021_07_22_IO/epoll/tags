Sock	Sock.hpp	/^class Sock$/;"	c
Socket	Sock.hpp	/^        static int Socket()$/;"	f	class:Sock
Start	EpollServer.hpp	/^        void Start()$/;"	f	class:EpollServer
bucket	EpollServer.hpp	/^        bucket(int sock)$/;"	f	class:bucket
bucket	EpollServer.hpp	/^class bucket$/;"	c
buffer	EpollServer.hpp	/^        char buffer[20];$/;"	m	class:bucket
epfd	EpollServer.hpp	/^        int epfd;  \/\/ epoll 模型的文件描述符，指向一个epoll结构体，结构体里有红黑树和双向链表，这些机制使得epoll模型非常高效$/;"	m	class:EpollServer
fd	EpollServer.hpp	/^        int fd;$/;"	m	class:bucket
lsock	EpollServer.hpp	/^        int lsock; \/\/ 监听套接字$/;"	m	class:EpollServer
main	main.cc	/^int main()$/;"	f
port	EpollServer.hpp	/^        int port;  \/\/ 端口号$/;"	m	class:EpollServer
pos	EpollServer.hpp	/^        int pos;$/;"	m	class:bucket
